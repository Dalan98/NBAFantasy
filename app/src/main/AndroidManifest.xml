<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.drproyects.nbafantasy">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:usesCleartextTraffic="true"
        android:icon="@drawable/kobe_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/kobe_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.NBAFantasy">
        <activity android:name="com.drproyects.nbafantasy.LoginActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CrearUsuarioActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.drproyects.nbafantasy.MainActivity"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>